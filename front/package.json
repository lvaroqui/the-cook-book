{
  "name": "front",
  "version": "1.0.0",
  "description": "Frontend",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"vite --host 0.0.0.0 --port 443 --https true\" \"yarn generate --watch\"",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "fix": "run-s fix:*",
    "fix:lint": "eslint src --ext ts,vue --fix src",
    "test": "run-s test:*",
    "test:lint": "eslint src --ext ts,vue src",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "pinia": ">=2.0.0-rc.6",
    "vue": "3.x",
    "vue-router": "4.x"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.1.2",
    "@graphql-codegen/typescript-graphql-request": "^4.1.2",
    "@graphql-codegen/typescript-operations": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "graphql-tag": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.9",
    "prettier": "^2.2.1",
    "tailwindcss": "2.x",
    "typescript": "^4.2.3",
    "vite": "^2.5.3",
    "vue-tsc": "^0.3.0"
  }
}
